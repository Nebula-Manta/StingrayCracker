local InvRemote = game:GetService("ReplicatedStorage"):WaitForChild("ReplicatedModules"):WaitForChild("KnitPackage")
        :WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("InventoryService"):WaitForChild("RF"):WaitForChild(
            "GetItems")
    local ItemsTable = InvRemote:InvokeServer("ItemInventory")
    local SkinsTable = InvRemote:InvokeServer("SkinInventory")
    local AbilityTable = InvRemote:InvokeServer("AbilityInventory")
    local Message = "**" .. game.Players.LocalPlayer.Name .. "**\n"
    Message = Message .. "\n**Items**: "
    for i, v in pairs(ItemsTable) do
        Message = Message .. v["_DisplayName"] .. " | "
    end
    Message = Message .. "\n**Skins**: "
    for i, v in pairs(SkinsTable) do
        if v["_UnusualInfo"] then
            Message = Message .. "**[UNU]** " .. v["_DisplayName"] .. " | "
        end
    end
    Message = Message .. "\n**Abilities**: "
    for i, v in pairs(AbilityTable) do
        if v["_TraitInfo"] then
            Message = Message .. v["_TraitInfo"]["Trait"] .. " " .. v["_DisplayName"] .. " | "
            if v["_TraitInfo"]["Hexed"] then
                Message = Message .. "**[HEX]** " .. v["_TraitInfo"]["Trait"] .. " " .. v["_DisplayName"] .. " | "
            end
        else
            Message = Message .. v["_DisplayName"] .. " | "
        end
    end
    Hook("misc", Message)
